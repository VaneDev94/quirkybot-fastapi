{
  "name": "react-tailwind-ts-starter",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://vanedev94.github.io/frontend-demobot/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:check": "tsc -b && vite build --mode development",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:verbose": "gh-pages -d dist --verbose",
    "clean": "rm -rf dist .tsc-cache node_modules/.vite",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.6.2",
    "vite": "^7.1.3"
  }
}